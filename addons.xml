<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.bslatinotv"
       name="BSLatinoTV"
       version="5.0.1"
       provider-name="BS Latino TV">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.1"/>
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.beautifulsoup4" />
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.requests" />
    <import addon="script.module.httplib2" />
    <import addon="script.module.youtube.dl" optional="true"/>
    <import addon="plugin.video.youtube" />
    <import addon="script.module.urlresolver" optional="true"/>
    <import addon="script.module.simplejson" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>basado en livestreamspro 1.8.5 by Shani</summary>
    <description>BS Latino TV</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.bslatinotv" name="BS Latino Tv official Repo" version="4.0.0" provider-name="BSLatino">
	<extension point="xbmc.addon.repository" name="BS Latino Tv official Repo">
		<info compressed="false">https://raw.githubusercontent.com/BSLatinoTV/bslatinotv/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/BSLatinoTV/bslatinotv/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/BSLatinoTV/bslatinotv/master</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>BS Latino Tv official Repo</summary>
   		<description lang="en">Instala los Addons de BS Latino TV.</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>
</addons>
